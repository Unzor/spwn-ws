websocket = (wsurl, msg) {
    url = "https://http-ws.seven7four4.repl.co/ws"
    body = '{"url":"{}","message":"{}"}'.fmt([wsurl, msg])
    method = "post"
    headers = {
        "Content-Type": "application/json",
    }
    return $.http_request(method, url, headers, body).text
}
return websocket
