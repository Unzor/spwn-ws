websocket = (wsurl, msgs) {
    url = "https://http-ws.seven7four4.repl.co/ws"
    body = '{"url":"{}","messages":"{}", "timeout":"{}"}'.fmt([wsurl, msgs.messages, msgs.timeout])
    method = "post"
    headers = {
        "Content-Type": "application/json",
    }
    return $.http_request(method, url, headers, body).text
}
return websocket
